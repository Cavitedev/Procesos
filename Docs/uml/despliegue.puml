@startuml Test Diagram
!theme mars
Node Navegador{
    file indexWeb as "index.html"
    file clienteRest as "clienteRest.js"
    file controlWeb as "controlWeb.js"
    file clienteWS as "clienteWS.js"
    file jquery as "jquery.js"


    indexWeb ..> jquery
    indexWeb ..> clienteRest
    indexWeb ..> controlWeb
    indexWeb ..> clienteWS

}
Node Server{
